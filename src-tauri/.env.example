# Cloudflare R2設定
# 以下の値を実際の値に置き換えてください

# CloudflareアカウントID
R2_ACCOUNT_ID=your_account_id_here

# R2 APIトークンのアクセスキー
R2_ACCESS_KEY_ID=your_access_key_here

# R2 APIトークンのシークレットキー
R2_SECRET_ACCESS_KEY=your_secret_key_here

# R2バケット名
R2_BUCKET_NAME=expense-receipts-dev

# R2リージョン（通常は"auto"）
R2_REGION=auto